<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.HomeActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.viewpager.widget.ViewPager
                    android:id="@+id/vpImg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="PENDAKERS"
                        android:textColor="@color/black"
                        android:textSize="22dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="Pendataan Perjanjian Kerjasama"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@color/underline" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/smd"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/smd1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/samarindalogo"
                        android:contextClickable="true"/>

                    <Button
                        android:id="@+id/smd2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Samarinda"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/balikpapan"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/bpp1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/balikpapanlogo"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/bpp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Balikpapan"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/bontang"
                    android:contextClickable="true"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <Button
                        android:id="@+id/btg1"
                        android:contextClickable="true"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/bontanglogo" />

                    <Button
                        android:id="@+id/btg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Bontang"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@color/underline" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/kutaikar"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:contextClickable="true" >

                    <Button
                        android:id="@+id/kukar1"
                        android:contextClickable="true"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/kukarlogo" />

                    <Button
                        android:id="@+id/kukar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Kutai Kartanegara"
                        android:textColor="@color/black"
                        android:textSize="13dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/kutaitim"
                    android:contextClickable="true"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <Button
                        android:id="@+id/kutim1"
                        android:contextClickable="true"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/kutimlogo" />

                    <Button
                        android:id="@+id/kutim"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Kutai Timur"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/kutaibar"
                    android:contextClickable="true"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <Button
                        android:id="@+id/kubar1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/kubarlogo"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/kubar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Kutai barat"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@color/underline" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/beraube"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/berau1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/beraulogo"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/berau"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="berau"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/paserpa"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/paser1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/paserlogo"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/paser"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="paser"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mahakamulu"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/mku1"
                        android:contextClickable="true"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/mulogo" />

                    <Button
                        android:id="@+id/mku"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="Mahakam Ulu"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@color/underline" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/penajam"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/penajam1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/ppulogo"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/ppu"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="penajam"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/kaltim"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:contextClickable="true">

                    <Button
                        android:id="@+id/kaltim1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/logoprovkaltim"
                        android:contextClickable="true" />

                    <Button
                        android:id="@+id/pemprov"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:text="prov kaltim"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:contextClickable="true" />
                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="2dp"
                android:background="@color/underline" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnLogout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="30dp"
                android:layout_marginRight="50dp"
                android:layout_marginBottom="30dp"
                android:background="@drawable/my_shapelogo"
                android:backgroundTint="#4fcd69"
                android:text="@string/logout"
                android:textColor="@color/white"

                app:layout_constraintBottom_toBottomOf="@+id/imageView"
                app:layout_constraintEnd_toEndOf="@+id/imageView"
                app:layout_constraintStart_toStartOf="@+id/imageView" />

        </LinearLayout>

    </ScrollView>

</RelativeLayout>